{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "dictionaries": [
    "software-terms",
    "npm",
    "fullstack",
    "redis",
    "bash",
    "typescript"
  ],
  "files": ["**", ".vscode/**", ".github/**"],
  "ignorePaths": ["bun.lock"],
  "ignoreRegExpList": ["apiKey='[a-zA-Z0-9-]{32}'"],
  "import": [
    "@cspell/dict-bash/cspell-ext.json",
    "@cspell/dict-npm/cspell-ext.json"
  ],
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "anirudh",
    "sriram",
    "techwithanirudh",
    "lefthook",
    "biome",
    "cspell",
    "commitlint",
    "devcontainers",
    "subsecond",
    "activedescendant",
    "noopener",
    "noninteractive",
    "powerlevel",
    "hass",
    "turbopack",
    "logomark",
    "bunx",
    "bprogress",
    "nosniff",
    "sameorigin",
    "sitemapindex",
    "katex",
    "nums",
    "dall",
    "daveyplate",
    "neondatabase",
    "Embla"
